def selection(arr):
    n = len(arr)
    for i in range(n):
        minimum_pos = i
        for j in range(i+1,n):
            if arr[j] < arr[minimum_pos]:
                minimum_pos = j
        temp_variable = arr[i]
        arr[i] = arr[minimum_pos]
        arr[minimum_pos] = temp_variable
    return arr

arr = [3,6,4,8,6,9]
print(selection(arr))

